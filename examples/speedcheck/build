DIST=$PILOTS_HOME/classes

java -Dsim -Dpackage=pilots.examples.speedcheck pilots.compiler.PilotsCompiler SpeedCheck.plt

echo "Compiling generated java code"
javac -Xlint:none -d $DIST SpeedCheck.java 

echo "Updating jar file..."
CURRENT=$PWD
cd $DIST
jar cf ../lib/pilots.jar `find pilots/compiler/` `find pilots/runtime/` `find pilots/util/` `find pilots/examples/`
cd $CURRENT

echo "Finished!"

